{
  "name": "usa-gaming-distributor-client",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "preview": "vite preview",
    "supabase": "supabase",
