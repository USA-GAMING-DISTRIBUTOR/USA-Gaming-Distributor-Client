    "db:gen": "supabase gen types typescript --linked > src/types/database.types.ts",
    "db:push": "supabase db push",
    "db:reset": "supabase db reset --force --debug",
    "db:studio": "supabase studio"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "@supabase/supabase-js": "^2.53.0",
    "@tailwindcss/vite": "^4.1.11",
    "@types/html2canvas": "^0.5.35",
    "@types/react-router-dom": "^5.3.3",
    "chart.js": "^4.5.0",
    "html2canvas": "^1.4.1",
    "lucide-react": "^0.534.0",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.7.1",
    "react-window": "^1.8.11",
    "tailwindcss": "^4.1.11",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@testing-library/jest-dom": "^6.6.3",
